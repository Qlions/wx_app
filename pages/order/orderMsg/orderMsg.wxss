/* pages/orderMsg/orderMsg.wxss */
.order_msg{
    display: flex;
    font-size: 26rpx;
    padding: 8rpx 0 12rpx 0;
    border-bottom: 2rpx solid #eee;
   
}
.service_img{
    position: relative;
    top: 0;
    bottom: 0;
    margin: auto 0 auto 0;
    height: 140rpx;
    width:  140rpx;
}
.service_name{
    color: #333;
}
.service_name, .service_ea, .order_view{
    margin-top: 10rpx;
}
.service_ea{
    display: block;
    color: #E5E5E5 ;
    font-size: 24rpx;
}
.service_ea_name{
    color: #999;
    margin: 0 8rpx 0 8rpx;
}
.service_money{
    float: right;
    color: #ff5454;
}
.service_class_image,.service_time_image{
    width: 32rpx;
    vertical-align:middle;
}
.service_class, .service_tiem{
    font-size: 24rpx;
    color: #bfbfbf;
    margin-left: 8rpx;
}
.service_doc_name_time{
    color: #999;
    font-size:24rpx;
}
.pay_mode, .service_total{
    line-height: 58rpx;
    color:#999;
    font-size:24rpx;
    border-bottom:2rpx solid #eee;
    
}
.service_timeScopeStr, {
    margin-left: 16rpx;
}
.service_doc{
    color: #ff5454;
    padding-right: 20rpx;
}
.service_doc_time{
    line-height: 58rpx;
    color:#999;
    font-size:24rpx;
}
.service_doc_name{
    line-height: 58rpx;
    color:#999;
    font-size:24rpx;
    border-bottom:2rpx solid #eee;
}
.orderState, .orderUserState{
    width:92%; 
    margin:0 auto;
    color: #999;
    font-size:24rpx;
}
.orderState{
    line-height: 58rpx;
}
.orderNumber, .downOrderTime, .user_msg{
    line-height: 58rpx;
}
.pay_mode_text, .orderState text:first-of-type, .service_total_text, .orderNumber text:first-of-type, .service_time_text, .service_doc_name text:first-of-type, .downOrderTime text:first-of-type, .user_msg text:first-of-type{
    color: #666;
    padding-right: 20rpx;
}
.pay_mode_int{
    float: right;
    padding: 0;
}
.pay_money_view{
    line-height: 80rpx;
    color: #ff5454;
    width: 92%;
    margin: 0 auto;
}
.cancel_btn{
    width: 18%;
    border: 2rpx solid #999;
    text-align:center;
}
.order_btn .time_count_down{
    color: #ff5454;
    font-size: 24rpx !important;
    float: left;
    width: 40%;
    position: absolute;
    left: 4%;
}
.pay_btn{
    width: 18%;
    text-align: center;
    background: #ff5454;
    color: #fff;
    border: 2rpx solid #ff5454;
}
.del_btn{
    width:18%;
    text-align:center;
    background:#ff5454;
    color:#fff;
    border:2rpx solid #ff5454; 
}
.order_btn text{
    font-size: 26rpx;
    border-radius: 10rpx;
    padding: 8rpx;
}
.order_btn {
    /* padding: 0 0 16rpx 0; */
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top: 2rpx solid #eee;
    display: flex;
    justify-content: flex-end;
    height: 80rpx;
}
page{
    height: 100%;
}
.evaluation{
    display: flex;
    width: 92%;
    height:auto;
    align-items:center;
    margin: 0 auto;
    justify-content: flex-end;
}
.star{
    display: flex;
    flex-grow: 1;
    height: 100%;
    align-items:center;
}
.star image{
    width: 28rpx;
}

.btn_box{
    width: 92%;
    margin: 0 auto;
    height: auto;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}


/* 取消订单 */
.order_cancel{
    position: fixed;
    bottom: 0;
    width: 100%;
    background: #fff;
    height: 55%;
    z-index: 11;
}
.close_text{
     line-height: 60rpx;
}
.cancel_list1{
    border-bottom: 2rpx solid #eee;
    font-size: 30rpx;
}
.cancel_list,.cancel_list1{
    height: 60rpx;
    width: 92%;
    margin: 0 auto;
    color: #666;
    font-size: 28rpx;
}
.cancel_list image {
    width: 28rpx;
    vertical-align: middle;
    padding-right: 14rpx;  
}
.cancel_list{
    line-height: 60rpx;
}
.close_{
    float: right;
    width: 36rpx;
    vertical-align: middle;
    margin-top: 12rpx;
}
.input {
    border: 2rpx solid #999;
    background: #eee;
    padding-left: 20rpx;
    width: 90%;
}
.cancel_ok{
    line-height: 80rpx;
    background: #63b64e;
    color: #fff;
    position: fixed;
    width: 100%;
    bottom: 0;
    font-size: 32rpx;
    text-align: center;
}

.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    bottom: 0;
    background: #000;
    opacity: 0.4;
    z-index: 10;
}

.big {
    width: 80%;
    background: #fff;
    border-radius: 16rpx;
    position: fixed;
    top: 20%;
    left: 10%;
    z-index: 12;
}
.icon_Exclamation{
    line-height: 100rpx;
    width: 100%;
    margin-top: 20rpx; 
}
.one{
    margin: 0 auto;
    width: 50rpx;
    line-height: 100rpx;
}
.one image{
    width: 100%;
}
.tip_cancel{
    font-size: 30rpx;
    color: #333;
    text-align: center;
    width: 80%;
    margin: 0 auto;
    padding-bottom: 20rpx;

}
.think_about{
    font-size: 28rpx;
    color: #666;
    text-align: center;
    line-height: 80rpx;
    border-top: 2rpx solid #eee;
}
.cancel_instructions{
    font-size: 28rpx;
    color: #666;
    text-align: center;
    line-height: 80rpx;
    border-top: 2rpx solid #eee;
}
.cancel_now{
    font-size: 28rpx;
    color: #ff7f10;
    text-align: center;
    line-height: 80rpx;
    border-top: 2rpx solid #eee;
}

/* 评价 */
.evaluation_model{
    width: 100%;
    height: 40%;
    background: #fff;
    position: fixed;
    bottom: 0;
    line-height: 60rpx;
    z-index: 12;
}
.evaluation_model_text{
    width: 92%;
    display: flex;
    margin: 0 auto;
}
.evaluation_model_text icon {
    position: relative;
    top: 22rpx;
}
.evaluation_model_text text{
    font-size: 28rpx;
    color: #666;
    text-align: center;
    flex-grow: 9;
}
.evaluation_model_star{
    display: flex;
    height: 60rpx;
    width: 92%;
    margin: 0 auto;
}
.z image{
    width: 40rpx;
    margin-top: 4rpx;
    margin-left: 42rpx;
}
.z image:first-of-type{
    margin-left: 0; 
}
.z{
    width: 50%;
    display: flex;
    margin: 0 auto;
}


.evaluation_model_btn{
    width: 100%;
    text-align: center;
    line-height: 80rpx;
    background: #63b64e;
    color: #fff;
    font-size: 30rpx;
    position: fixed;
    bottom: 0;
}

.evaluation_model_textarea {
    width: 92%;
    margin: 0 auto;
    height: 240rpx;
}
.evaluation_model_textarea textarea{
    width: 100%;
    height: 100%;
    font-size: 28rpx;
    color: #333;
    line-height: 50rpx;
}