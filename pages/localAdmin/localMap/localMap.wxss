.local_map_seachr {
  background: #eee;
  font-size: 14px;
  overflow:hidden;
  display:-webkit-box;
  display:-webkit-flex;
  -webkit-box-align:center;
  -webkit-align-items:center;
  text-align:left;
  border-radius:40px;
  margin-bottom:20rpx;
}
.local_map_seachr image {
  width: 32rpx;
  height: 32rpx;
  padding:20rpx;
}
.local_map_seachr input {
  -webkit-box-flex:1;
  -webkit-flex:1;
}
.local_map_list {
  font-size: 14px;
  padding: 0 20rpx; 
}
.local_map_list .item {
  padding-top: 25rpx;
}
.local_map_list .name {
  color: #333;
}
.local_map_list .address {
  color: #999;
  font-size: 12px;
  padding-top: 20rpx;
}
.local_map_sealist {
  font-size: 14px;
  padding: 0 20rpx; 
}
.local_map_sealist_item {
  padding-top: 25rpx;
}
.local_map_sealist_item .name {
  color: #333;
}
.local_map_sealist_item .address {
  color: #999;
  font-size: 12px;
  padding-top: 20rpx;
}
